{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:webview:default",
    "core:app:default",
    "core:resources:default",
    "core:image:default",
    "core:menu:default",
    "core:tray:default",
    "sql:allow-select",
    "sql:allow-close",
    "sql:allow-load",
    "sql:allow-execute",
    "fs:write-all",
	"fs:read-all",
	{
		"identifier": "fs:scope",
		"allow": [{ "path": "**" }]
	}
  ]
}